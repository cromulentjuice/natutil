{
  "version": 2,
  "functions": {
    "api/convert.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "flask-backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "flask-backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "flask-backend"
  }
}
